{
  "name": "react-electron",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.4",
    "sequelize": "^4.39.0",
    "sqlite3": "^4.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.0.1",
    "electron": "^3.0.2",
    "electron-builder": "^20.28.4",
    "react-desktop": "^0.3.7",
    "wait-on": "^3.1.0",
    "electron-rebuild": "^1.8.2"
  }
}
